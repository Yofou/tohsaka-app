{
  "name": "@tohsaka/api-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "graphql-codegen",
    "build": "pnpm deploy --filter \".\" apps/api/dist",
