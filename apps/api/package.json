    "@urql/core": "^2.5.0",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "isomorphic-fetch": "^3.0.0",
    "zod": "^3.17.3",
    "@ariesclark/eslint-config": "^1.4.0",
    "@graphql-codegen/typescript": "^2.5.1",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "18.0.1",
    "eslint": "8.19.0",
    "tsc-watch": "^5.0.3",
    "typescript": "4.7.4"
  }
