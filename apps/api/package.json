{
  "name": "@tohsaka/api-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "generate": "graphql-codegen",
    "build": "tsc",
    "start:dev": "docker compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d && tsc-watch --onSuccess \"node ./dist/index.js\"",
    "start": "pnpm build && docker compose up -d && node ./dist/index.ts",
    "stop": "docker compose down",
    "lint": "next lint --fix"
  },
  "dependencies": {
    "18h": "^3.0.13",
    "@graphql-codegen/cli": "^2.6.4",
    "@tohsaka/types": "workspace:*",
    "@trpc/server": "^9.25.3",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@ariesclark/eslint-config": "^1.4.0",
    "@graphql-codegen/typescript": "^2.5.1",
    "@types/express": "^4.17.13",
    "@types/node": "18.0.1",
    "eslint": "8.19.0",
    "tsc-watch": "^5.0.3",
    "typescript": "4.7.4"
  }
}
